{
    "name": "wnx/faker-swiss-cities",
    "description": "This is my package faker-swiss-cities",
    "keywords": [
        "wnx",
        "faker-swiss-cities"
    ],
    "homepage": "https://github.com/wnx/faker-swiss-cities",
    "license": "MIT",
    "authors": [
        {
            "name": "Stefan Zweifel",
            "email": "hello@stefanzweifel.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "pestphp/pest": "^1.20",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Wnx\\FakerSwissCities\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wnx\\FakerSwissCities\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
