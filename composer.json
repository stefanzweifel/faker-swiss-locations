{
    "name": "wnx/faker-swiss-locations",
    "description": "Provider to generate valid Swiss location data for Faker PHP. ",
    "keywords": [
        "wnx",
        "faker",
        "switzerland",
        "swiss",
        "cities",
        "fake-data"
    ],
    "homepage": "https://github.com/stefanzweifel/faker-swiss-locations",
    "license": "MIT",
    "authors": [
        {
            "name": "Stefan Zweifel",
            "email": "stefan@stefanzweifel.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "fakerphp/faker": "^1.20",
        "wnx/php-swiss-cantons": "^5.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "pestphp/pest": "^3.7",
        "phpstan/phpstan": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Wnx\\FakerSwissLocations\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wnx\\FakerSwissLocations\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
